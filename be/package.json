{
  "name": "task-taking-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -e .env.prod -- ts-node src/app.ts",
    "prepare:prod": "yarn flushdb:prod &&  yarn migrate && yarn seed",
    "prepare:test": "yarn flushdb:test && yarn migrate:test && yarn seed:test",
    "migrate": "dotenv -e .env.prod -- npx sequelize-cli db:migrate",
    "migrate:test:status": "dotenv -e .env.test -- npx sequelize-cli db:migrate:status",
    "migrate:prod:status": "dotenv -e .env.prod -- npx sequelize-cli db:migrate:status",
    "test:clear": "yarn undo:migrate:test && yarn undo:seed:test",
    "migrate:test": "dotenv -e .env.test -- npx sequelize-cli db:migrate",
    "undo:migrate": "dotenv -e .env.prod -- npx sequelize-cli db:migrate:undo:all",
    "undo:migrate:test": "dotenv -e .env.test -- npx sequelize-cli db:migrate:undo:all",
    "seed": "dotenv -e .env.prod -- npx sequelize-cli db:seed:all",
    "seed:test": "dotenv -e .env.test -- npx sequelize-cli db:seed:all",
    "undo:seed": "dotenv -e .env.prod -- npx sequelize-cli db:seed:undo:all",
    "undo:seed:test": "dotenv -e .env.test -- npx sequelize-cli db:seed:undo:all",
    "flushdb:test": "dotenv -e .env.test -- sequelize-cli db:drop && dotenv -e .env.test -- sequelize-cli db:create",
    "flushdb:prod": "dotenv -e .env.prod -- sequelize-cli db:drop && dotenv -e .env.prod -- sequelize-cli db:create",
    "pretest": "yarn flushdb:test && yarn prepare:test",
    "test": "dotenv -e .env.test -- jest --runInBand --forceExit",
    "clean": "npx rimraf  node_modules package-lock.json  yarn.lock && yarn",
    "format": "prettier --check \"**/*.{ts,js}\""
  },
  "packageManager": "yarn@1.20.0+",
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "private": true,
  "devDependencies": {
    "jest": "^27.0.6",
    "supertest": "^6.3.3"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.4.9",
    "@types/supertest": "^2.0.12",
    "babel": "^6.23.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.1",
    "http-status": "^1.6.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.0",
    "prettier": "^3.0.1",
    "redis": "^4.6.7",
    "rimraf": "^5.0.1",
    "sequelize": "^6.32.1",
    "sequelize-cli": "^6.6.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
